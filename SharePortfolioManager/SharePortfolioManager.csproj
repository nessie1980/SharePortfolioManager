    <PreBuildEvent>REM Update Revision number

echo "$(ConfigurationName)"

if "$(ConfigurationName)" == "Release_Revision"  (
$(ProjectDir)\BuildTools\AssemblyInfoUpdater.exe REVISION $(ProjectDir)Properties\AssemblyInfo.cs
)

if "$(ConfigurationName)" == "Release_Build" (
$(ProjectDir)\BuildTools\AssemblyInfoUpdater.exe BUILD $(ProjectDir)Properties\AssemblyInfo.cs
)

if "$(ConfigurationName)" == "Release_Minor" (
$(ProjectDir)\BuildTools\AssemblyInfoUpdater.exe MINOR $(ProjectDir)Properties\AssemblyInfo.cs
)

if "$(ConfigurationName)" == "Release_Major" (
$(ProjectDir)\BuildTools\AssemblyInfoUpdater.exe MAJOR $(ProjectDir)Properties\AssemblyInfo.cs
)

REM Copy settings files if they do not exist already
ECHO Copy settings
ECHO.
XCOPY "$(ProjectDir)Config\Settings.xml" "$(TargetDir)\Settings\" /Y /D
XCOPY "$(ProjectDir)Config\WebSites.xml" "$(TargetDir)\Settings\" /Y /D
XCOPY "$(ProjectDir)Config\Language.xml" "$(TargetDir)\Settings\" /Y /D
XCOPY "$(ProjectDir)Config\Documents.xml" "$(TargetDir)\Settings\" /Y /D

ECHO Copy DLLs
ECHO.



XCOPY "$(ProjectDir)DLLs\*.*" "$(TargetDir)" /Y /D
</PreBuildEvent>
